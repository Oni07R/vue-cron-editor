{
  "name": "vue-cron-editor",
  "version": "0.1.7",
  "main": "dist/vue-cron-editor.umd.js",
  "module": "dist/vue-cron-editor.esm.js",
  "unpkg": "dist/vue-cron-editor.min.js",
  "browser": {
    "./sfc": "src/vue-cron-editor.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-cron-editor src/VueCronEditor.vue",
    "build:samples": "vue-cli-service build",
    "lint": "vue-cli-service lint --ext .js,.vue,.ts --fix",
    "test": "vue-cli-service test:unit",
    "release:samples": "npm run build:samples && gh-pages -d dist",
    "release": "standard-version"
  },
  "dependencies": {
    "@types/jest": "^24.0.25",
    "core-js": "^3.6.1",
    "gh-pages": "^2.1.1",
    "vue": "^2.6.11",
    "vuetify": "^2.1.15"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "standard-version": "^7.0.1",
    "typescript": "~3.5.3",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karoletrych/vue-cron-editor.git"
  },
  "files": [
    "dist"
  ]
}
